<p id="notice"><%= notice %></p>

<h1>Ecategories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ecategories.each do |ecategory| %>
      <tr>
        <td><%= ecategory.name %></td>
        <td><%= link_to 'Show', ecategory %></td>
        <td><%= link_to 'Edit', edit_ecategory_path(ecategory) %></td>
        <td><%= link_to 'Destroy', ecategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ecategory', new_ecategory_path %>
